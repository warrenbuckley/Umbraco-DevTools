<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <appSettings>
        <!-- Doing this in two steps (remove, then insert) ensures that we can update an existing value, or add a new one -->
        <add key="owin:appStartup" xdt:Transform="Remove" xdt:Locator="Match(key)"/>
        <add key="owin:appStartup" value="UmbracoDefaultOwinStartup" xdt:Transform="Insert" xdt:Locator="Match(key)"/>
    </appSettings>
</configuration>